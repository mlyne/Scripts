s/\/\#/::SYM/g;
s/\/\$/::N/g;
s/\/\'\'/::SCM/g;
s/\/\(/::SCM/g;
s/\/\)/::SCM/g;
s/\/\,/::EOP/g;
s/\/\-\-/::EOP/g;
s/\/\./::EOS/g;
s/\/:/::EOP/g;
s/\/CC/::CJ/g;
s/\/CD/::CD/g;
s/\/EX/::P/g;
s/\/FW/::UK/g;
s/\/IN/::IN/g;
s/\/JJR/::J/g;
s/\/JJSS/::J/g;
s/\/JJS/::J/g;
s/\/JJ/::J/g;
s/\/LS/::CD/g;
s/\/MD/::V/g;
s/\/NNPS/::NP/g;
s/\/NNP/::NP/g;
s/\/NNS/::N/g;
s/\/NN/::N/g;
s/\/PDT/::D/g;
s/\/POS/::P/g;
s/\/PRP\$/::D/g;
s/\/PRP/::P/g;
s/\/RBR/::R/g;
s/\/RBS/::R/g;
s/\/SYM/::SYM/g;
s/\/TO/::IN/g;
s/\/UH/::UH/g;
s/\/VBD/::V/g;
s/\/VBG/::VBG/g;
s/\/VBN/::VBN/g;
s/\/VBP/::V/g;
s/\/VBZ/::V/g;
s/\/VB/::V/g;
s/\/WDT/::D/g;
s/\/WP\$/::P/g;
s/\/WP/::P/g;
s/\/WRB/::R/g;
s/\/\`\`/::SCM/g;
s/\/RB/::R/g;
